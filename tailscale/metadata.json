{
    "desktop-plugin-version": "1.0.0-beta.1",
    "name":"tailscale",
    "provider": "Tailscale Inc.",
    "icon":"tailscale.svg",
    "vm": {
        "image":"${DESKTOP_PLUGIN_IMAGE}"
    },
    "ui":{
        "dashboard-tab":
        {
            "title":"Tailscale",
            "root":"/ui",
            "src":"index.html"
        }
    },
    "host": {
        "binaries": [
            {
                "darwin": [
                    {
                        "path": "/darwin/hostname"
                    },
                    {
                        "path": "/darwin/tsbrowser"
                    }
                ],
                "windows": [
                    {
                        "path": "/windows/hostname.cmd"
                    },
                    {
                        "path": "/windows/tsbrowser.cmd"
                    }
                ]
            }
        ]
    }
}
