{
  "desktop-plugin-version": "1.0",
  "name": "telepresence",
  "ui": {
    "dashboard-tab": {
      "title": "Telepresence",
      "root": "/ui",
      "src": "index.html"
    }
  },
  "host": {
    "binaries": [
      {
        "darwin": [
          {
            "path": "/darwin/telepresence"
          },
          {
            "path": "/darwin/kubectl"
          }
        ],
        "windows": [
          {
            "path": "/windows",
            "command": "powershell",
            "args": [
              "-Command",
              "Start-Process",
              "PowerShell",
              "-Verb",
              "RunAs",
              "-ArgumentList",
              "'-Command','cd C:\\Users\\felipe\\AppData\\Roaming\\Docker\\Desktop\\plugins\\telepresence\\host\\windows; ./telepresence-win-install.ps1'",
              "-Wait"
            ]
          }
        ]
      }
    ]
  }
}
