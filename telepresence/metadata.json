{
  "desktop-plugin-version": "1.0",
  "name": "telepresence",
  "ui": {
    "dashboard-tab": {
      "title": "Telepresence",
      "root": "/ui",
      "src": "index.html"
    }
  },
  "host": {
    "binaries": [
      {
        "darwin": {
          "path": "/darwin/telepresence"
        },
        "windows": {
          "assetsPath": "/windows",
          "script": "telepresence-win-install.ps1",
          "command": "powershell -Command Start-Process PowerShell -Verb RunAs -ArgumentList '-NoExit','-Command',"
        }
      },
      {
        "darwin": {
          "path": "/darwin/kubectl"
        }
      }
    ]
  }
}
