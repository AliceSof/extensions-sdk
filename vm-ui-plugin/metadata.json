{
    "desktop-plugin-version":"1.0",
    "name":"volume-sample",
    "provider": "Docker Inc.",
    "vm": {
        "composefile":"docker-compose.yaml",
        "isolation":"docekrd",
        "exposes":{"socket": "plugin-volumes.sock", "host-socket": "plugin-volumes-host.sock", "named-pipe": "\\\\.\\pipe\\dockerPluginVolumes"}
    },
    "ui":{
        "dashboard-tab":
        {
            "title":"Volumes",
            "root":"/plugin-ui",
            "src":"index.html",
            "backend":{"host-socket": "plugin-volumes-host.sock", "named-pipe": "\\\\.\\pipe\\dockerPluginVolumes"}
        }
    }
}